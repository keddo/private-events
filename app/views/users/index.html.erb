<div class="table-container">
  <h1 class="title is-centered"> All Users List </h1>
  <table class="table">
    <tr>
     <th>Id</th>
     <th>Name</th>
     <th>Email</th>
     <th>Action</th>
    </tr>
   <% @users.each do |user| %>
    <tr>
       <td><%= user.id %> </td>
       <td><%= user.name %> </td>
       <td><%= user.email %> </td>
    </tr>
    <% end %>
  </table>
</div>